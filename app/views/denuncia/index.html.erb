<p id="notice"><%= notice %></p>

<h1>Denuncia</h1>

<table>
  <thead>
    <tr>
      <th>Tipo pe persona</th>
      <th>Nombre</th>
      <th>Ci</th>
      <th>Ru item</th>
      <th>Fecha</th>
      <th>Facultad</th>
      <th>Carrera</th>
      <th>Tipo resol</th>
      <th>Codigo caso</th>
      <th>Direccion</th>
      <th>Telefono</th>
      <th>Observaciones</th>
      <th>Fallo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @denuncia.each do |denuncium| %>
      <tr>
        <td><%= persona.tipo_pe_persona %></td>
        <td><%= persona.nombre %></td>
        <td><%= persona.ci %></td>
        <td><%= persona.ru_item %></td>
        <td><%= persona.fecha %></td>
        <td><%= persona.facultad_id %></td>
        <td><%= persona.carrera_id %></td>
        <td><%= persona.tipo_resol %></td>
        <td><%= persona.codigo_caso %></td>
        <td><%= persona.direccion %></td>
        <td><%= persona.telefono %></td>
        <td><%= persona.observaciones %></td>
        <td><%= persona.fallo %></td>
        <td><%= link_to 'Show', denuncium %></td>
        <td><%= link_to 'Edit', edit_denuncium_path(denuncium) %></td>
        <td><%= link_to 'Destroy', denuncium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Denuncium', new_denuncium_path %>
